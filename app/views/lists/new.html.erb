<h1>新規投稿</h1>
<!-- from_with -->
<!--< %= form_with model: @list do |f| %> -->
<%= form_with model: @list, url: "/lists", method: :post do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  
  <h4>本文</h4>
  <%= f.text_area :body %>

  <h4>画像</h4>
  <!-- ファイルフィールド :image, 受け入れ"image/*" -->
  <%= f.file_field :image, accept: "image/*" %>

  <%= f.submit "投稿" %>

<% end %>

<p><%= link_to "トップ", "/top" %></p>
<p><%= link_to "投稿一覧", "/lists" %></p>